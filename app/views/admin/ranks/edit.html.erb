<%= form_with model: [:admin, @rank] do |f| %>
  <div class="mb-3">
    <%= f.label :name, "名称" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :position, "並び順(小さいほど上)" %>
    <%= f.number_field :position, class: "form-control" %>
  </div>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
